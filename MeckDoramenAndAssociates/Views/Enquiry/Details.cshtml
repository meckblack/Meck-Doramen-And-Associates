@model MeckDoramenAndAssociates.Models.Enquiry

@{
    ViewData["Title"] = "Details";
    Layout = null;
}

<div class="modal-header">
    <h3>
        DETAIL OF ENQUIRY
    </h3>
</div>

<div class="modal-body">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-widget">
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-6">
                            <p><b>Name:</b></p>
                            <p>@Html.DisplayFor(model => model.FullName)</p>
                        </div>
                        <div class="col-md-3">
                            <p><b>Email:</b></p>
                            <p>@Html.DisplayFor(model => model.Email)</p>
                        </div>
                        <div class="col-md-3">
                            <p><b>Phone Number:</b></p>
                            <p>@Html.DisplayFor(model => model.PhoneNumber)</p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <p><b>Message:</b></p>
                            <p>@Html.DisplayFor(model => model.Message)</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <a class="btn btn-danger" data-dismiss="modal">Cancel</a>
</div>